#include<bits/stdc++.h>
using namespace std;

int main(){

    int n;
    cin >> n;

    for(int i=0 ; i< n; i++)
    {
        char c;
        cin >> c;

         if (c=='c' or c=='o' or c=='d' or c=='e' or c=='f' or c=='r' or c=='s')
         {
             cout << "Yes" <<endl;
         }
         else
         {
             cout<<"NO" <<endl;
         }
    }
    return 0;
}
