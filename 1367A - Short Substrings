#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;

    for(int i = 0; i < n; i++)
    {
        string s;
        cin>>s;

        cout<<s[0];

        int len = s.length();
        for(int i=1; i<len; i=i+2)
        {
            cout <<s[i];
        }
        cout<<endl;
    }
}
