#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;

    for(int i = 0 ; i<n; i++)
    {
        int x;
        cin >> x;

        string name;
        cin >> name;

        sort(name.begin(), name.end());

        if (x != 5)
        {
            cout << "NO" << endl;
        }


        else if (name == "Timru")
        {
            cout << "Yes" << endl;
        }
        else
        {
            cout << "NO" << endl;
        }
    }
    return 0;
}
