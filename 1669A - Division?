#include <iostream>
using namespace std;

int main()
{
    int n, rate;
    cin >> n;
    for(int i = 0; i < n; i++)
    {
        cin >> rate;
        if(rate >= 1900)
        {
            cout << "Division 1" << endl;
        }
        else if(rate >= 1600 && rate <= 1899)
        {
            cout << "Division 2" << endl;
        }
        else if(rate >= 1400 && rate <= 1599)
        {
            cout << "Division 3" << endl;
        }
        else
        {
            cout << "Division 4" << endl;
        }
    }
    return 0;
}



