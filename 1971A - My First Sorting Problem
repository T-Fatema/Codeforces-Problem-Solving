#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;

    for (int i = 0; i<n; i++)
    {
        int x , y ;
        cin >> x >> y ;

        if ( x > y)
        {
            cout<< y << " "<< x << endl ;
        }
        else
        {
            cout << x << " " << y << endl;
        }
    }

    return 0;

}
